    "jest": "^22.1.4",
    "lite-server": "^2.3.0",
    "shelljs": "^0.7.7",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.0.2",
    "tslint": "^5.8.0",
    "webpack": "^3.10.0"
  }
